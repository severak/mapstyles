<?xml version="1.0" encoding="UTF-8"?>
<rendertheme xmlns="http://mapsforge.org/renderTheme" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://mapsforge.org/renderTheme ../renderTheme.xsd" version="1" map-background="#F8F8F8">
<rule e="any" k="*" v="*" zoom-max="12" >
<!-- landuse -->
<rule e="way" k="natural|landuse|amenity" v="forest|wood|grave_yard">
	<area fill="#E8EEC3" stroke-width="0.000"/>
	<line stroke="#000000" stroke-width="1.000" stroke-dasharray="1, 6"/>
</rule>

<rule e="way" k="landuse|leisure|sport" v="grass|meadow|vineyard|orchad|village_green||park|golf">
	<area fill="#F1F6E7" stroke-width="0.000"/>
	<line stroke="#000000" stroke-width="1.000" stroke-dasharray="1, 6"/>
</rule>

<rule e="way" k="landuse" v="residential">
	<area fill="#BBBCBC" stroke-width="0.000"/>
	<line stroke="#000000" stroke-width="1.000"/>
</rule>

<!-- contours -->
<rule e="way" k="contour_ext" v="elevation_major">
	<line stroke="#E99222" stroke-width="2.000"/>
</rule>

<rule e="way" k="contour_ext" v="elevation_medium|elevation_minor" zoom-min="14">
	<line stroke="#E99222" stroke-width="1.000"/>
</rule>

<!-- water -->
<rule e="way" k="waterway" v="stream|canal|ditch|drain" zoom-min="14">
	<line stroke="#1980AA" stroke-width="1.000"/>
</rule>

<rule e="way" k="waterway" v="river" zoom-min="8">
	<line stroke="#1980AA" stroke-width="2.000"/>
</rule>

<rule e="way" k="natural|landuse" v="water|reservoir">
	<area fill="#C9E7F6" stroke="#1980AA" stroke-width="1.000"/>
</rule>

<rule e="way" k="waterway" v="riverbank">
	<area fill="#C9E7F6" stroke="#1980AA" stroke-width="1.000"/>
</rule>

<!-- buildings -->
<rule e="way" k="building" v="*">
	<area fill="#000000" stroke-width="0.000"/>
</rule>

<!-- barriers -->
<rule e="way" k="barrier" v="city_wall|fence|retaining_wall|wall">
	<line stroke="#000000" stroke-width="1.000"/>
	<line stroke="#000000" stroke-width="3.000" stroke-dasharray="1, 4" stroke-linecap="butt"/>
</rule>

<!-- power lines -->
<rule e="way" k="power" v="line" zoom-min="10">
	<line stroke="#000000" stroke-width="1.000"/>
</rule>
<rule e="node" k="power" v="tower" zoom-min="10">
	<circle radius="3" stroke="#000000" stroke-width="1.000"/>
</rule>
	
<!-- airports -->
<rule e="way" k="aeroway" v="*" zoom-min="10">
	<line stroke="#000000" stroke-width="1.000"/>
</rule>

<!-- ways -->
<rule e="way" k="tunnel" v="~|no">
<rule e="way" k="bridge" v="~|no">
<rule e="way" k="area" v="~|no">

	<!-- casing -->
	<rule e="way" k="highway" v="track|path|bridleway" zoom-min="14">
		<line stroke="#000000" stroke-width="1.000" stroke-dasharray="12, 4"/>
	</rule>
	
	<rule e="way" k="highway" v="residential|road|service|unclassified" zoom-min="13">
		<line stroke="#000000" stroke-width="4.000" stroke-linecap="butt"/>
	</rule>
	
	<rule e="way" k="highway" v="motorway|trunk|primary|secondary|tertiary|motorway_link|trunk_link|primary_link|secondary_link|tertiary_link">
		<line stroke="#000000" stroke-width="4.000" stroke-linecap="butt"/>
	</rule>

	<!-- stroke -->
	<rule e="way" k="highway" v="residential|road|service|unclassified" zoom-min="13">
		<line stroke="#FFFFFF" stroke-width="2.000" stroke-linecap="butt"/>
	</rule>
	
	<rule e="way" k="highway" v="motorway|trunk|primary|secondary|tertiary|motorway_link|trunk_link|primary_link|secondary_link|tertiary_link">
		<line stroke="#E99222" stroke-width="2.000" stroke-linecap="butt"/>
	</rule>
	

	<rule e="way" k="railway" v="rail|narrow_gauge">
		<rule e="way" k="service" v="yard">
			<!-- todo rozfungovat vedlejsi koleje -->
			<line stroke="#000000" stroke-width="1.000" stroke-linecap="butt"/>
		</rule>
		<rule e="way" k="service" v="mainline|~">
			<line stroke="#000000" stroke-width="2.000" stroke-linecap="butt"/>
		</rule>
	</rule>
</rule>
</rule>
</rule>

<!-- bridges -->

<rule e="way" k="bridge" v="*">
	<rule e="way" k="highway" v="*">
		<line stroke="#000000" stroke-width="4.000" stroke-linecap="butt"/>
		<line stroke="#FFFFFF" stroke-width="2.000" stroke-linecap="butt"/>
	</rule>
	
	<rule e="way" k="railway" v="rail|narrow_gauge">
		<line stroke="#000000" stroke-width="5.000" stroke-linecap="butt"/>
		<line stroke="#FFFFFF" stroke-width="3.000" stroke-linecap="butt"/>
	</rule>
</rule>



</rule>

