{
  "version": 8,
  "name": "Vincent (Shortbread)",
  "metadata": {},
  "center": [14, 50],
  "zoom": 14,
  "sources": {
    "shortbread_v1": {
      "type": "vector",
      "url": "https://vector.openstreetmap.org/shortbread_v1/tilejson.json"
    }
  },
  "sprite": "",
  "glyphs": "https://orangemug.github.io/font-glyphs/glyphs/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {"background-color": "#eee8d5"}
    },
    {
      "id": "forest",
      "type": "fill",
      "source": "shortbread_v1",
      "source-layer": "land",
      "filter": [
        "all",
        ["in", "kind", "forest", "scrub", "orchard", "park", "cemetery"]
      ],
      "paint": {"fill-color": "#CAEDAB"}
    },
    {
      "id": "ocean",
      "type": "fill",
      "source": "shortbread_v1",
      "source-layer": "ocean",
      "filter": ["all"],
      "paint": {"fill-color": "#7BAFDE"}
    },
    {
      "id": "water",
      "type": "fill",
      "source": "shortbread_v1",
      "source-layer": "water_polygons",
      "paint": {"fill-color": "#7BAFDE"}
    },
    {
      "id": "waterway",
      "type": "line",
      "source": "shortbread_v1",
      "source-layer": "water_lines",
      "filter": ["all", ["!=", "tunnel", true]],
      "paint": {"line-color": "#7BAFDE", "line-width": 2}
    },
    {
      "id": "buildings",
      "type": "fill",
      "source": "shortbread_v1",
      "source-layer": "buildings",
      "paint": {"fill-color": "#D6C1DE"}
    },
    {
      "id": "residential",
      "type": "fill",
      "source": "shortbread_v1",
      "source-layer": "land",
      "maxzoom": 12,
      "filter": ["all", ["==", "kind", "residential"]],
      "paint": {"fill-color": "#D6C1DE"}
    },
    {
      "id": "track",
      "type": "line",
      "source": "shortbread_v1",
      "source-layer": "streets",
      "filter": [
        "all",
        ["in", "kind", "track", "path", "footway", "unclassified"]
      ],
      "paint": {"line-dasharray": [2, 2], "line-color": "#F6C141"}
    },
    {
      "id": "minor road",
      "type": "line",
      "source": "shortbread_v1",
      "source-layer": "streets",
      "filter": [
        "all",
        [
          "in",
          "kind",
          "living_street",
          "service",
          "road",
          "residential",
          "cycleway",
          "pedestrian",
          "taxiway"
        ]
      ],
      "paint": {"line-color": "#F6C141", "line-opacity": 1}
    },
    {
      "id": "major roads",
      "type": "line",
      "source": "shortbread_v1",
      "source-layer": "streets",
      "filter": [
        "all",
        [
          "in",
          "kind",
          "link",
          "motorway",
          "motorway_link",
          "primary",
          "secondary",
          "tertiary",
          "trunk",
          "runway"
        ],
        ["!=", "tunnel", true]
      ],
      "paint": {"line-color": "#F6C141", "line-width": 2}
    },
    {
      "id": "subway",
      "type": "line",
      "source": "shortbread_v1",
      "source-layer": "streets",
      "filter": ["all", ["==", "kind", "subway"]],
      "paint": {"line-dasharray": [3, 3]}
    },
    {
      "id": "railway",
      "type": "line",
      "source": "shortbread_v1",
      "source-layer": "streets",
      "filter": [
        "all",
        ["in", "kind", "rail", "narrow_gauge", "funicular"],
        ["!=", "tunnel", true]
      ]
    },
    {
      "id": "tram",
      "type": "line",
      "source": "shortbread_v1",
      "source-layer": "streets",
      "filter": ["all", ["==", "kind", "tram"]],
      "paint": {"line-color": "#ff0000"}
    },
    {
      "id": "railway stop symbol",
      "type": "circle",
      "source": "shortbread_v1",
      "source-layer": "public_transport",
      "filter": ["all", ["in", "kind", "station", "halt"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "circle-color": "#000",
        "circle-stroke-color": "#fff",
        "circle-stroke-width": 2,
        "circle-radius": 4
      }
    },
    {
      "id": "tram or bus stop symbol",
      "type": "circle",
      "source": "shortbread_v1",
      "source-layer": "public_transport",
      "filter": ["all", ["in", "kind", "tram_stop", "bus_stop"]],
      "paint": {
        "circle-radius": 2,
        "circle-color": "#ff0000",
        "circle-stroke-color": "#ffffff",
        "circle-stroke-width": 1
      }
    },
    {
      "id": "water names",
      "type": "symbol",
      "source": "shortbread_v1",
      "source-layer": "water_polygons_labels",
      "minzoom": 13,
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["Noto Sans Italic"],
        "text-size": 13
      },
      "paint": {
        "text-halo-color": "#fff",
        "text-color": "#7BAFDE",
        "text-halo-width": 2,
        "text-halo-blur": 1
      }
    },
    {
      "id": "waterway names",
      "type": "symbol",
      "source": "shortbread_v1",
      "source-layer": "water_lines_labels",
      "layout": {
        "text-font": ["Noto Sans Italic"],
        "text-field": ["get", "name"],
        "symbol-placement": "line",
        "text-size": 10
      },
      "paint": {
        "text-halo-color": "#fff",
        "text-halo-width": 2,
        "text-halo-blur": 1,
        "text-color": "#7BAFDE"
      }
    },
    {
      "id": "secondary place names",
      "type": "symbol",
      "source": "shortbread_v1",
      "source-layer": "place_labels",
      "filter": ["all", ["in", "kind", "hamlet", "suburb", "quarter"]],
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["Noto Sans Regular"],
        "text-size": 10
      },
      "paint": {
        "text-halo-color": "#fff",
        "text-halo-width": 2,
        "text-halo-blur": 1
      }
    },
    {
      "id": "places",
      "type": "symbol",
      "source": "shortbread_v1",
      "source-layer": "place_labels",
      "filter": [
        "all",
        ["in", "kind", "capital", "state_capital", "city", "town", "village"]
      ],
      "layout": {
        "text-font": ["Noto Sans Regular"],
        "text-field": ["get", "name"],
        "text-transform": "uppercase",
        "text-size": 13
      },
      "paint": {
        "text-halo-width": 2,
        "text-halo-color": "#fff",
        "text-halo-blur": 1
      }
    },
    {
      "id": "street name",
      "type": "symbol",
      "source": "shortbread_v1",
      "source-layer": "street_labels",
      "minzoom": 14,
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["Noto Sans Regular"],
        "symbol-placement": "line",
        "symbol-avoid-edges": false,
        "text-size": 10,
        "text-allow-overlap": false,
        "symbol-z-order": "auto",
        "text-ignore-placement": false,
        "text-transform": "none",
        "text-optional": false
      },
      "paint": {
        "text-halo-color": "#fff",
        "text-halo-width": 5,
        "text-halo-blur": 1
      }
    },
    {
      "id": "tram or bus stop name",
      "type": "symbol",
      "source": "shortbread_v1",
      "source-layer": "public_transport",
      "filter": ["all", ["in", "kind", "tram_stop", "bus_stop"]],
      "layout": {
        "text-font": ["Noto Sans Regular"],
        "text-field": ["get", "name"],
        "text-size": 10,
        "text-anchor": "left",
        "icon-allow-overlap": false,
        "icon-ignore-placement": false,
        "text-allow-overlap": false,
        "text-ignore-placement": false,
        "text-justify": "auto",
        "text-pitch-alignment": "auto",
        "symbol-avoid-edges": false,
        "symbol-z-order": "source",
        "symbol-placement": "point"
      },
      "paint": {
        "text-translate": [4, 0],
        "text-color": "#ff0000",
        "text-halo-width": 1,
        "text-halo-color": "#ffffff"
      }
    },
    {
      "id": "railway stop name",
      "type": "symbol",
      "source": "shortbread_v1",
      "source-layer": "public_transport",
      "filter": ["all", ["in", "kind", "station", "halt"]],
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["Noto Sans Regular"],
        "text-justify": "auto",
        "text-anchor": "left",
        "text-size": 13,
        "text-transform": "none",
        "text-allow-overlap": false,
        "text-ignore-placement": false,
        "visibility": "visible"
      },
      "paint": {
        "text-color": "#000",
        "text-halo-color": "#fff",
        "text-halo-width": 5,
        "text-translate": [7, 0],
        "icon-opacity": 1,
        "text-halo-blur": 0
      }
    }
  ],
  "id": "80twwnc"
}